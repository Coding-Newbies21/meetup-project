<h1 class="centerTitle">My Events</h1>
{{#each eventsArr}}
<div class="list-elm">
  <img class="event-img" src="{{this.image}}">
  <br><br>
  <div class="test">
     <h3>{{this.title}}</h3>
     <a href="/events/{{this._id}}">More details</a>
     <br><br>
      <form action="/events/{{this._id}}/delete" method="POST" >
       <button class="redirect-btn">Delete</button>
    </form>
  </div>
 
</div>
{{/each}}














    


